<section id="{{title}}">
    <h2>{{title}}</h2>
    <div class="result-container">
        {% for result in results %}
        <h2>[{{result.tier}}]</h3>
            {% for opponent in result.headToHeads %}
            <div class="opponent-container">
                <span class="count">x{{opponent.count}}</span>
                <span class="tag">{{opponent.name}}</span>
                <span class="separator">,</span>
            </div>
            {% endfor %}
            {% endfor %}
    </div>
</section>